      activeSetString = String(ActiveFanControlSet);
    }
    display->drawString(128, 51, String(F("Auto: ")) + (settings.AutomaticFanControlActive ? F("Set ") : F("off")) + activeSetString);
    
    display->display();
  }
  displayIsOn = turnOn;
}

