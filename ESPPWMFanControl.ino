  double R1 = 150000.0;
  double Beta = 3950.0;
  double To = 298.15;
  double Ro = 10000.0;

  double adcMax = 1023.0;
  double Vs = 3.3;

  double Vout = 3.3, Rt = 0;
  double T, Tc;

  Vout = analogRead(A0) * Vs / adcMax;
  Rt = R1 * Vout / (Vs - Vout);
  T = 1 / (1 / To + log(Rt / Ro) / Beta);
  Tc = T - 273.15; // temp in Celsius
  Tc = round(Tc * 100) / 100; // round to 2 decimals

  return Tc;
}

